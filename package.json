{
    "name": "ts-add-js-extension",
    "description": "Add .js extension to each relative import and export in JavaScript file",
    "version": "1.2.3",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "license": "MIT",
    "type": "module",
    "bin": {
        "ts-add-js-extension": "bin/ts-add-js-extension.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "setup": "yarn install --frozen-lockfile",
        "build": "rm -rf build && tsc",
        "pretest": "yarn build && cd test && rm -rf output && mkdir output && cp -r sample/* output",
        "test": "node test"
    },
    "bugs": "https://github.com/P-YNPM/ts-add-js-extension/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/P-YNPM/ts-add-js-extension"
    },
    "devDependencies": {
        "@types/node": "^16.11.10",
        "@types/yargs": "^17.0.12",
        "typescript": "^4.8.3"
    },
    "dependencies": {
        "@typescript-eslint/typescript-estree": "^5.36.2",
        "console-table-printer": "^2.11.1",
        "yargs": "^17.5.1"
    },
    "keywords": [
        "js extension",
        "esnext"
    ]
}
